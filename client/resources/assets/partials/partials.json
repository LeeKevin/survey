{"admin-button":"Button!","dashboard":"<div class=\"modal-header\"> <h2>Survey Admin</h2> <a class=\"logout\">logout</a> </div> <div class=\"modal-content-area\"> {{#if asTemplate}} <a class=\"survey-admin-back\">Back</a> <div class=\"survey-admin-dashboard-area\"></div> {{else}} <a class=\"create-question\">Add Question</a> <div id=\"active-questions-container\"></div> <div id=\"archived-questions-container\"></div> {{/if}} </div> ","edit-question":"<form class=\"edit-question-form\"> <label for=\"edit-question-value\">Question</label> <input id=\"edit-question-value\" class=\"edit-question-value\" name=\"question\" type=\"text\" value=\"{{question}}\"/> <label>Options</label> <a class=\"add-question-item\" title=\"Add an option\"></a> <div class=\"question-items-list\"> {{#QuestionItems}} {{> question-item-editable }} {{/QuestionItems}} </div> <input type=\"submit\" value=\"Save\"/> <span id=\"edit-question-error\" class=\"error\" style=\"display: none;\"></span> </form>","login":"<span class=\"login-header\">Survey Admin Log In</span> <form id=\"admin-login-form\"> <label for=\"survey-login-username\">Username</label> <input id=\"survey-login-username\" name=\"username\" type=\"text\"/> <label for=\"survey-login-password\">Password</label> <input id=\"survey-login-password\" name=\"password\" type=\"password\"/> <input type=\"submit\" value=\"Log In\"/> </form>","modal":"<div class=\"modal-overlay\"> <a class=\"modal-close\" title=\"Close\"></a> <div class=\"modal-content\"> </div> </div>","question-item-editable":"<div class=\"question-item-container\"> <input type=\"text\" class=\"question-item\" data-id=\"{{id}}\" value=\"{{option}}\"/> <a class=\"remove-question-item\" title=\"Remove this option\"></a> </div>","question":"<span class=\"info\">Survey</span> <span class=\"header\">{{question}}</span> <ul class=\"question-items\"> {{#QuestionItems}} <li class=\"question-item\" data-id=\"{{id}}\">{{option}}</li> {{/QuestionItems}} </ul> <a class=\"question-submit\">Submit</a>","questions-list":"<span class=\"survey-header {{#minimized}}minimized{{/minimized}}\" data-count=\"{{count}}\">{{status}} questions</span> <ul class=\"dashboard-question-list\"> {{#questions}} <li> <div class=\"question\" data-id=\"{{id}}\"> <a title=\"{{question}}\">{{question}}</a> <div class=\"question-actions\"> {{^archived}} <a class=\"question-edit fa fa-pencil {{#if AnswerTotal}}disabled{{/if}}\" title=\"{{#if AnswerTotal}}This question cannot be edited because it has already been answered{{else}}Edit this question{{/if}}\"></a> <a class=\"question-archive fa fa-archive\" title=\"Archive this question\"></a> {{/archived}} </div> </div> <div class=\"question-details\"> <div class=\"total-count\">Count: {{AnswerTotal}}</div> {{#QuestionItems}} <div class=\"option-container\"> {{option}} <span class=\"option-percent\">{{percent AnswerCount ../AnswerTotal}}</span> </div> {{/QuestionItems}} </div> </li> {{/questions}} </ul> ","thanks":"<span class=\"thanks\">Thank you!</span>"}